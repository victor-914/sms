      <div id="role-list"></div>

      <script type="module">
        import { data } from "./data.js";

        const roleList = document.getElementById("role-list");

        // Function to load roles from the JSON file
        function loadRoles(data) {
          data.roles.forEach((role) => {
            // Create the main container for each role
            const roleContainer = document.createElement("div");
            roleContainer.className = "role-container";

            // Left side: image and title
            const imageContainer = document.createElement("div");
            imageContainer.className = "image-container";

            const img = document.createElement("img");
            img.src = role.image;
            img.alt = role.title;

            imageContainer.appendChild(img);

            // Right side: features
            const contentContainer = document.createElement("div");
            contentContainer.className = "content-container";

            const title = document.createElement("h2");
            title.textContent = role.title;
            contentContainer.appendChild(title);

            // Loop through the features and add them to the content container
            role.features.forEach((feature) => {
              const featureDiv = document.createElement("div");
              featureDiv.className = "feature";

              const subtitle = document.createElement("h4");
              subtitle.textContent = feature.subtitle;
              featureDiv.appendChild(subtitle);

              const content = document.createElement("p");
              content.textContent = feature.content;
              featureDiv.appendChild(content);

              contentContainer.appendChild(featureDiv);
            });

            // Append the image and content containers to the main role container
            roleContainer.appendChild(imageContainer);
            roleContainer.appendChild(contentContainer);

            // Append the role to the list
            roleList.appendChild(roleContainer);
          });
        }

        // Call the loadRoles function with the data imported from data.js
        loadRoles(data);
      </script>
